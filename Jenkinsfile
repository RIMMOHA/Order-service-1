pipeline {
    agent any

    stages {
        stage1('mvn Build') {
            steps {
                echo 'Building..'
            }
        }
        stage2('mvn Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage3('mvn Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
